{
  "name": "terraform-provider-tokend",
  "version": "1.0.0",
  "dependencies": {
    "@tokend/redoc-cli": "^0.8.1",
    "swagger-repo": "^2.0.0-rc.11"
  },
  "private": true,
  "scripts": {
    "start": "swagger-repo serve",
    "build": "swagger-repo build -o web_deploy",
    "test": "swagger-repo validate",
    "release-build": "npm run build && redoc-cli bundle ./web_deploy/openapi.yaml  -o ../public/index.html -t ./web/index.hbs --options.theme.typography.links.color=#695de4 --options.item-types-instead-of-operations=true --options.root-param-name-as-group-header=true --options.hide-download-button=true --options.hide-response-samples=true --options.hide-path=true --options.flatten-response-view=true --options.code-samples-instead-of-request-samples=true",
    "gh-pages": "swagger-repo gh-pages"
  }
}
